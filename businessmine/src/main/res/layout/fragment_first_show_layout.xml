<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="first_vm"
            type="com.young.businessmine.ui.viewmodel.FirstShowVM" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.young.commomlib.widget.PageStatusView
            android:id="@+id/loading_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:status_img_source="@drawable/akatsuki"
            app:status_text_size="13sp"
            app:status_text_color="@color/colorAccent"></com.young.commomlib.widget.PageStatusView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>